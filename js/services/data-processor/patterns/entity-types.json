{
  "person": {
    "keywords": ["person", "individual", "human", "contact", "employee", "customer", "user"],
    "properties": ["first_name", "last_name", "gender", "age", "birth", "dob", "phone", "email"],
    "fieldPatterns": {
      "name": {
        "regex": "^[A-Z][a-z]+ [A-Z][a-z]+$",
        "fields": ["name", "fullname", "full_name"]
      },
      "gender": {
        "values": ["male", "female", "m", "f"],
        "fields": ["gender", "sex"]
      },
      "email": {
        "regex": "^[^@]+@[^@]+\\.[^@]+$",
        "fields": ["email", "mail"]
      }
    }
  },
  "organization": {
    "keywords": ["company", "organization", "org", "business", "corporation", "enterprise", "firm", "agency", "institution"],
    "properties": ["industry", "sector", "employees", "founded", "headquarters", "revenue", "website"],
    "fieldPatterns": {
      "suffix": {
        "values": ["inc", "corp", "llc", "ltd", "gmbh", "co", "company", "corporation", "associates", "partners"],
        "fields": ["name", "company_name", "title"]
      },
      "industry": {
        "fields": ["industry", "sector", "category", "type"]
      },
      "founded": {
        "regex": "^\\d{4}$",
        "fields": ["founded", "year", "established", "started"]
      }
    }
  },
  "location": {
    "keywords": ["location", "place", "address", "venue", "site", "position", "building", "city", "town", "country"],
    "properties": ["latitude", "longitude", "address", "city", "state", "country", "postal_code", "zip"],
    "fieldPatterns": {
      "coordinates": {
        "fields": ["latitude", "longitude", "lat", "lng", "coordinates"]
      },
      "address": {
        "fields": ["address", "street", "place"]
      },
      "city": {
        "fields": ["city", "town", "locale"]
      },
      "country": {
        "fields": ["country", "nation"]
      }
    }
  },
  "event": {
    "keywords": ["event", "meeting", "conference", "session", "gathering", "activity"],
    "properties": ["date", "start_date", "end_date", "location", "attendees", "title"],
    "fieldPatterns": {
      "date": {
        "fields": ["date", "time", "start", "end", "when"]
      },
      "duration": {
        "fields": ["duration", "length"]
      },
      "venue": {
        "fields": ["venue", "location", "place"]
      }
    }
  },
  "vehicle": {
    "keywords": ["vehicle", "car", "truck", "auto", "automobile"],
    "properties": ["make", "model", "year", "plate", "vin", "color"],
    "fieldPatterns": {
      "make": {
        "fields": ["make", "manufacturer", "brand"]
      },
      "model": {
        "fields": ["model"]
      },
      "plate": {
        "fields": ["plate", "license_plate", "registration"]
      }
    }
  }
}
